
<%= form_for @rider do |f| %>
  <%= f.label :name %>:
  <%= f.text_field :name %><br />

  <%= f.label :age %>:
  <%= f.text_field :age %><br />

  <%= f.label :skill_level %>:
  <%= f.text_field :skill_level %><br />

  <%= f.label :height %>:
  <%= f.text_field :height %><br />

  <%= f.submit %>
<% end %>